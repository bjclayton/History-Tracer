{
    "app-config": {

        "windows":{
            "title":"History Tracer",
            "iconSrc": "resources/images/History.png",
            "databaseNmae": "jdbc:sqlite:windowsDatabase.sqlite"
        },

        "linux":{
            "title":"History Tracer",
            "iconSrc": "resources/images/History.png",
            "databaseNmae": "jdbc:sqlite:linuxDatabase.sqlite"
        },

        "os": ["Windows", "Linux"],

        "chrome": {
            "name": "Chrome",
            "iconSrc": "images/chrome.png",
            "databasePath": "C:\\Users\\%s\\AppData\\Local\\Google\\Chrome\\User Data\\Default\\History",
            "sqlCommand": ["SELECT * FROM %s", "SELECT * FROM urls WHERE title LIKE '% %s %'"],
            "siteFields": ["url", "title", "last_visit_time", "visit_count"],
            "downloadFields": ["referrer", "current_path", "total_bytes"],
            "loginFields": ["origin_url", "username_value"]
        },

        "popupDetail":{
            "title": "Detail",
            "labels": ["Title", "Url", "Last visit time", "Visit count"],
            "iconSrc": "resources/images/History.png"
        }

    }   
}  